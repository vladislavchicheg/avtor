.modal
  input[type="radio"]
    &+label
      &:after,&:before
        content none !important
      .helper-
        &outer
          position absolute
          size 17px
          border 1px solid #f8ac59
          border-radius 50%
          left 0
          top 0
          transition .3s
        &inner
          position absolute
          size 17px
          background-color #f8ac59
          border-radius 50%
          left 0
          top -1px
          transition .3s
          transform scale(0)
